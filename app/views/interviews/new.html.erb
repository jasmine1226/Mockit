<%= form_with model: @interview, local: true do |f|%>
    
    <%= f.label :interviewer_id %>
    <%= f.select :interviewer_id, Interviewer.all.collect { |i| [ i.name, i.id ] }, include_blank: true %><br>
    <%= f.label :date %>
    <%= f.date_field :date %><br>
    <%= f.label :time %>
    <%= f.time_field :time %><br>    
    <%= f.label "Length (hr)" %>
    <%= f.number_field :length %><br>


    <%= f.submit "Schedule Interview" %>
<% end %>