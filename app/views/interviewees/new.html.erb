<% if !session[:auth] %>
    <%= button_to 'Sign up with Facebook!', '/auth/facebook/?account_type=interviewee', :class => "btn  btn-primary"  %><br>
<% end %>

<%= bootstrap_form_with model: @interviewee, local: true do |f|%>

    <%= render 'form', f: f %>

    <%= f.submit "Create Account", :class => "btn  btn-primary"  %>
<% end %>
