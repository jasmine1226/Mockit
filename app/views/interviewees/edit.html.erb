<%= bootstrap_form_for(@interviewee) do |f|%>

    <%= render 'form', f: f %>

    <%= f.submit "Update Account", :class => "btn  btn-primary"  %>
<% end %>

<% if current_user == @interviewee%>
    <p><%= button_to 'Delete Account', interviewee_path(@interviewee), method: :delete, :class => "btn  btn-secondary", data: {confirm: "Are you sure?"} %></p>
<% end %>