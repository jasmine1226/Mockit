    <%= f.error_messages%>
    
    <% if session[:auth] %>
        <%= f.hidden_field :uid, value: @interviewee.uid %>
        <%= f.hidden_field :image, value: @interviewee.image %>
    <% end %>

    <%= f.text_field :name %> <br>
    <%= f.text_field :email %> <br>
    <%= f.password_field :password %> <br>
    <%= f.password_field :password_confirmation %> <br>
    <%= f.label "I aspire to become" %>
    <%= f.text_field :job_title, hide_label: true %> <br>
    <%= f.select :job_level, options_for_select([["Select One", ""], "Student", "Junior", "Senior", "Principal", "Director", "C-suite"], selected = @interviewee.job_level) %> <br>
    <%= f.label "Years of Experience" %>
    <%= f.number_field :experience, hide_label: true%> <br> 
    <%= f.number_field :balance %> <br><br>


