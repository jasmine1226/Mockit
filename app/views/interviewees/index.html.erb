
<% if !!flash[:error] %>
<% flash[:error].each do |e| %>
    <p><%= e %></p> <br>
    <% end %>
<% end %>

<h1>Interviewee Index Page</h1><br>
<% @interviewees.each do |interviewee| %>
    <%= link_to interviewee.name, interviewee_path(interviewee) %> - <%= interviewee.email %><br>
<% end %>

